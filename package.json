{
  "name": "checktypo",
  "version": "1.0.3",
  "description": "A package to check for typos on your website!",
  "main": "main.js",
  "readme": "README.md",
  "scripts": {
    "test": "mocha",
    "pretty": "prettier --write --tab-width 2 \"helpers/*.js\"",
    "validate": "npm-run-all --parallel test pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashishdommety/checktypo.git"
  },
  "author": "Ashish Dommety",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashishdommety/checktypo/issues"
  },
  "homepage": "https://github.com/ashishdommety/checktypo#readme",
  "dependencies": {
    "clear": "^0.1.0",
    "cli-spinner": "^0.2.8",
    "nightmare": "^3.0.0",
    "typo-js": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "husky": "^1.0.0-rc.2",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "request": "^2.85.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  }
}
